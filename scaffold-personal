"""æ•°æ®å¤„ç†æ¨¡å—"""

import pandas as pd
from pathlib import Path

def load_data(file_path: str) -> pd.DataFrame:
    """åŠ è½½æ•°æ®"""
    return pd.read_csv(file_path)

def preprocess_data(df: pd.DataFrame) -> pd.DataFrame:
    """æ•°æ®é¢„å¤„ç†"""
    # åœ¨è¿™é‡Œæ·»åŠ æ•°æ®é¢„å¤„ç†é€»è¾‘

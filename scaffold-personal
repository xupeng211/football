#!/bin/bash
# ğŸš€ ä¸ªäººé¡¹ç›®åˆå§‹åŒ–è„šæ‰‹æ¶
# åŠŸèƒ½ï¼šå¿«é€Ÿåˆ›å»ºæ–°é¡¹ç›®ï¼Œå¤ç”¨ç°æœ‰çš„ä¼˜ç§€é…ç½®

set -euo pipefail

# è„šæœ¬é…ç½®
SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
TEMPLATES_DIR="${SCRIPT_DIR}/project-templates"

# é¢œè‰²è¾“å‡º
readonly GREEN='\033[0;32m'
readonly BLUE='\033[0;34m'
readonly YELLOW='\033[1;33m'
readonly PURPLE='\033[0;35m'
readonly NC='\033[0m'

# æ˜¾ç¤ºæ¬¢è¿ä¿¡æ¯
show_welcome() {
    clear
    echo -e "${PURPLE}ğŸš€ ä¸ªäººé¡¹ç›®åˆå§‹åŒ–è„šæ‰‹æ¶${NC}"
    echo "=========================================="
    echo "å¿«é€Ÿåˆ›å»ºæ–°é¡¹ç›®ï¼Œå¤ç”¨ç°æœ‰çš„ä¼˜ç§€é…ç½®"
    echo ""
}

# æ˜¾ç¤ºå¸®åŠ©
show_help() {
    echo -e "${BLUE}ç”¨æ³•:${NC}"
    echo "  ./scaffold-personal <é¡¹ç›®å> [æ¨¡æ¿ç±»å‹]"
    echo ""
    echo -e "${BLUE}å¯ç”¨æ¨¡æ¿:${NC}"
    echo "  python-api      Python APIé¡¹ç›® (FastAPI + å®Œæ•´é…ç½®)"
    echo "  python-ml       æœºå™¨å­¦ä¹ é¡¹ç›® (æ•°æ®ç§‘å­¦ + MLflow)"
    echo "  python-basic    åŸºç¡€Pythoné¡¹ç›®"
    echo "  web-full        å…¨æ ˆWebé¡¹ç›® (å‰åç«¯åˆ†ç¦»)"
    echo ""
    echo -e "${BLUE}ç¤ºä¾‹:${NC}"
    echo "  ./scaffold-personal my-api python-api"
    echo "  ./scaffold-personal ml-project python-ml"
    echo "  ./scaffold-personal simple-tool python-basic"
    echo ""
}

# è·å–é¡¹ç›®æ¨¡æ¿åˆ—è¡¨
get_available_templates() {
    echo "python-api python-ml python-basic web-full"
}

# äº¤äº’å¼é€‰æ‹©æ¨¡æ¿
interactive_template_selection() {
    echo -e "${YELLOW}è¯·é€‰æ‹©é¡¹ç›®æ¨¡æ¿:${NC}"
    echo ""
    echo "1. ğŸ”¥ Python APIé¡¹ç›® (FastAPI + å®Œæ•´DevOpsé…ç½®)"
    echo "   - FastAPIæ¡†æ¶ + PostgreSQL + Redis + Docker"
    echo "   - å®Œæ•´CI/CD + æµ‹è¯•è¦†ç›– + ç›‘æ§"
    echo ""
    echo "2. ğŸ§  æœºå™¨å­¦ä¹ é¡¹ç›® (æ•°æ®ç§‘å­¦ + AIå·¥å…·)"
    echo "   - Jupyter + MLflow + æ¨¡å‹ç®¡ç†"
    echo "   - æ•°æ®å¤„ç† + å®éªŒè¿½è¸ª + éƒ¨ç½²"
    echo ""
    echo "3. ğŸ åŸºç¡€Pythoné¡¹ç›® (è½»é‡çº§)"
    echo "   - åŸºç¡€é…ç½® + æµ‹è¯• + æ–‡æ¡£"
    echo "   - é€‚åˆå·¥å…·ã€è„šæœ¬ã€åº“å¼€å‘"
    echo ""
    echo "4. ğŸŒ å…¨æ ˆWebé¡¹ç›® (å‰åç«¯åˆ†ç¦»)"
    echo "   - Reactå‰ç«¯ + Pythonåç«¯"
    echo "   - å®Œæ•´çš„Webåº”ç”¨æ¶æ„"
    echo ""

    read -p "è¯·é€‰æ‹© (1-4): " choice

    case $choice in
        1) echo "python-api" ;;
        2) echo "python-ml" ;;
        3) echo "python-basic" ;;
        4) echo "web-full" ;;
        *) echo "python-basic" ;;
    esac
}

# åˆ›å»ºé¡¹ç›®
create_project() {
    local project_name="$1"
    local template_type="$2"
    local target_dir="../$project_name"

    echo -e "${BLUE}ğŸš€ åˆ›å»ºé¡¹ç›®: ${project_name}${NC}"
    echo -e "${BLUE}ğŸ“‹ ä½¿ç”¨æ¨¡æ¿: ${template_type}${NC}"
    echo ""

    # æ£€æŸ¥ç›®æ ‡ç›®å½•
    if [[ -d "$target_dir" ]]; then
        echo -e "${YELLOW}âš ï¸  ç›®å½• $target_dir å·²å­˜åœ¨${NC}"
        read -p "æ˜¯å¦è¦†ç›–? (y/N): " confirm
        if [[ ! $confirm =~ ^[Yy]$ ]]; then
            echo "æ“ä½œå·²å–æ¶ˆ"
            exit 0
        fi
        rm -rf "$target_dir"
    fi

    # åˆ›å»ºé¡¹ç›®ç›®å½•
    mkdir -p "$target_dir"
    cd "$target_dir"

    # æ ¹æ®æ¨¡æ¿ç±»å‹å¤åˆ¶æ–‡ä»¶
    case $template_type in
        "python-api")
            create_python_api_project "$project_name"
            ;;
        "python-ml")
            create_python_ml_project "$project_name"
            ;;
        "python-basic")
            create_python_basic_project "$project_name"
            ;;
        "web-full")
            create_web_full_project "$project_name"
            ;;
        *)
            echo -e "${YELLOW}æœªçŸ¥æ¨¡æ¿ç±»å‹ï¼Œä½¿ç”¨åŸºç¡€Pythonæ¨¡æ¿${NC}"
            create_python_basic_project "$project_name"
            ;;
    esac

    # é¡¹ç›®åˆ›å»ºå®Œæˆ
    echo ""
    echo -e "${GREEN}ğŸ‰ é¡¹ç›®åˆ›å»ºå®Œæˆï¼${NC}"
    echo -e "${GREEN}ğŸ“ é¡¹ç›®ä½ç½®: ${target_dir}${NC}"
    echo ""
    echo -e "${BLUE}ğŸš€ ä¸‹ä¸€æ­¥æ“ä½œ:${NC}"
    echo "  cd $project_name"
    echo "  poetry install"
    echo "  git init && git add . && git commit -m 'Initial commit'"
    echo ""
}

# åˆ›å»ºPython APIé¡¹ç›®
create_python_api_project() {
    local project_name="$1"

    echo "ğŸ“¦ åˆ›å»ºPython APIé¡¹ç›®..."

    # å¤åˆ¶æ ¸å¿ƒé…ç½®æ–‡ä»¶
    cp "$SCRIPT_DIR/pyproject.toml" .
    cp "$SCRIPT_DIR/.gitignore" .
    cp "$SCRIPT_DIR/Makefile" .

    # å¤åˆ¶CI/CDé…ç½®
    mkdir -p .github/workflows
    cp "$SCRIPT_DIR/.github/workflows/ci.yml" .github/workflows/
    cp "$SCRIPT_DIR/.pre-commit-config.yaml" .

    # å¤åˆ¶Dockeré…ç½®
    cp "$SCRIPT_DIR/Dockerfile" .
    cp "$SCRIPT_DIR/docker-compose.yml" .

    # å¤åˆ¶è„šæœ¬
    mkdir -p scripts
    cp "$SCRIPT_DIR/scripts/activate-venv.sh" scripts/
    cp "$SCRIPT_DIR/scripts/setup-dev-env.sh" scripts/
    cp "$SCRIPT_DIR/scripts/ci-unified.sh" scripts/

    # åˆ›å»ºé¡¹ç›®ç»“æ„
    mkdir -p src/${project_name//-/_}
    mkdir -p tests
    mkdir -p docs

    # åˆ›å»ºä¸»è¦Pythonæ–‡ä»¶
    cat > "src/${project_name//-/_}/__init__.py" << EOF
"""${project_name} - APIé¡¹ç›®"""

__version__ = "0.1.0"
EOF

    cat > "src/${project_name//-/_}/main.py" << EOF
"""FastAPIåº”ç”¨ä¸»æ–‡ä»¶"""

from fastapi import FastAPI
from fastapi.middleware.cors import CORSMiddleware

app = FastAPI(
    title="${project_name}",
    description="APIé¡¹ç›®",
    version="0.1.0",
)

app.add_middleware(
    CORSMiddleware,
    allow_origins=["*"],
    allow_credentials=True,
    allow_methods=["*"],
    allow_headers=["*"],
)

@app.get("/")
async def root():
    return {"message": "Hello from ${project_name}!"}

@app.get("/health")
async def health_check():
    return {"status": "healthy"}

if __name__ == "__main__":
    import uvicorn
    uvicorn.run(app, host="0.0.0.0", port=8000)
EOF

    # åˆ›å»ºæµ‹è¯•æ–‡ä»¶
    cat > "tests/test_main.py" << EOF
"""æµ‹è¯•ä¸»åº”ç”¨"""

from fastapi.testclient import TestClient
from src.${project_name//-/_}.main import app

client = TestClient(app)

def test_root():
    response = client.get("/")
    assert response.status_code == 200
    assert response.json() == {"message": "Hello from ${project_name}!"}

def test_health_check():
    response = client.get("/health")
    assert response.status_code == 200
    assert response.json() == {"status": "healthy"}
EOF

    # åˆ›å»ºREADME
    cat > README.md << EOF
# ${project_name}

Python APIé¡¹ç›®ï¼Œä½¿ç”¨FastAPIæ¡†æ¶ã€‚

## å¿«é€Ÿå¼€å§‹

\`\`\`bash
# å®‰è£…ä¾èµ–
poetry install

# å¯åŠ¨å¼€å‘æœåŠ¡å™¨
poetry run uvicorn src.${project_name//-/_}.main:app --reload

# è¿è¡Œæµ‹è¯•
poetry run pytest

# ä»£ç æ£€æŸ¥
make lint

# æ ¼å¼åŒ–ä»£ç 
make format
\`\`\`

## APIæ–‡æ¡£

å¯åŠ¨æœåŠ¡åè®¿é—®ï¼š
- Swagger UI: http://localhost:8000/docs
- ReDoc: http://localhost:8000/redoc

## éƒ¨ç½²

\`\`\`bash
# Dockeréƒ¨ç½²
docker-compose up -d
\`\`\`
EOF

    # æ›´æ–°pyproject.tomlä¸­çš„é¡¹ç›®å
    sed -i "s/football-predict-system/${project_name}/g" pyproject.toml

    echo "âœ… Python APIé¡¹ç›®ç»“æ„åˆ›å»ºå®Œæˆ"
}


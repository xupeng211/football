def test_main():
    """æµ‹è¯•ä¸»å‡½æ•°"""
    # è¿™é‡Œæ·»åŠ å…·ä½“çš„æµ‹è¯•é€»è¾‘
    assert True
EOF

    # åˆ›å»ºREADME
    cat > README.md << EOF
# ${project_name}

åŸºç¡€Pythoné¡¹ç›®ã€‚

## å®‰è£…

\`\`\`bash
poetry install
\`\`\`

## ä½¿ç”¨

\`\`\`bash
poetry run python -m src.${project_name//-/_}.main
\`\`\`

## æµ‹è¯•

\`\`\`bash
poetry run pytest
\`\`\`
EOF

    # æ›´æ–°pyproject.toml
    sed -i "s/football-predict-system/${project_name}/g" pyproject.toml

    echo "âœ… åŸºç¡€Pythoné¡¹ç›®ç»“æ„åˆ›å»ºå®Œæˆ"
}

# åˆ›å»ºå…¨æ ˆWebé¡¹ç›®
create_web_full_project() {
    local project_name="$1"

    echo "ğŸ“¦ åˆ›å»ºå…¨æ ˆWebé¡¹ç›®..."

    # åˆ›å»ºå‰åç«¯åˆ†ç¦»ç»“æ„
    mkdir -p {backend,frontend,docs}

    # åç«¯è®¾ç½®
    cd backend
    create_python_api_project "${project_name}-backend"
    cd ..

    # å‰ç«¯è®¾ç½®ï¼ˆåˆ›å»ºåŸºç¡€ç»“æ„ï¼‰
    mkdir -p frontend/{src,public}

    cat > frontend/package.json << EOF
{
  "name": "${project_name}-frontend",

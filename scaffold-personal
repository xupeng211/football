# ä¸»å‡½æ•°
main() {
    # è§£æå‚æ•°
    case "${1:-}" in
        "-h"|"--help"|"help")
            show_help
            exit 0
            ;;
        "")
            show_welcome
            echo -e "${YELLOW}è¯·æä¾›é¡¹ç›®åç§°${NC}"
            echo ""
            read -p "é¡¹ç›®åç§°: " project_name
            if [[ -z "$project_name" ]]; then
                echo "é¡¹ç›®åç§°ä¸èƒ½ä¸ºç©º"
                exit 1
            fi
            template_type=$(interactive_template_selection)
            ;;
        *)
            project_name="$1"
            template_type="${2:-}"

            if [[ -z "$template_type" ]]; then
                show_welcome
                template_type=$(interactive_template_selection)
            fi
            ;;
    esac

    # éªŒè¯æ¨¡æ¿ç±»å‹
    available_templates=$(get_available_templates)
    if [[ ! " $available_templates " =~ " $template_type " ]]; then
        echo -e "${YELLOW}âš ï¸  æœªçŸ¥æ¨¡æ¿ç±»å‹: $template_type${NC}"
        echo "å¯ç”¨æ¨¡æ¿: $available_templates"
        template_type=$(interactive_template_selection)
    fi

    # åˆ›å»ºé¡¹ç›®
    create_project "$project_name" "$template_type"
}

# è„šæœ¬å…¥å£
main "$@"

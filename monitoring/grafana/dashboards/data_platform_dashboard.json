{
    "dashboard": {
        "id": null,
        "title": "Football Data Platform Dashboard",
        "description": "Monitoring dashboard for football data platform",
        "tags": [
            "football",
            "data-platform",
            "monitoring"
        ],
        "timezone": "browser",
        "panels": [
            {
                "id": 1,
                "title": "API Request Rate",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(http_requests_total[5m])",
                        "refId": "A"
                    }
                ]
            },
            {
                "id": 2,
                "title": "Data Collection Status",
                "type": "stat",
                "targets": [
                    {
                        "expr": "data_collection_success_total",
                        "refId": "A"
                    }
                ]
            },
            {
                "id": 3,
                "title": "Database Connection Pool",
                "type": "graph",
                "targets": [
                    {
                        "expr": "pg_stat_database_numbackends",
                        "refId": "A"
                    }
                ]
            },
            {
                "id": 4,
                "title": "Cache Hit Rate",
                "type": "graph",
                "targets": [
                    {
                        "expr": "redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total)",
                        "refId": "A"
                    }
                ]
            }
        ],
        "time": {
            "from": "now-1h",
            "to": "now"
        },
        "refresh": "30s"
    }
}
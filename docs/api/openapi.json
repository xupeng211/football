{
  "openapi": "3.1.0",
  "info": {
    "title": "足球预测API",
    "description": "足球比赛结果预测服务",
    "version": "1.0.0"
  },
  "paths": {
    "/api/v1/predict/single": {
      "post": {
        "tags": [
          "Predictions"
        ],
        "summary": "Predict Single Match",
        "description": "单场比赛预测",
        "operationId": "predict_single_match_api_v1_predict_single_post",
        "parameters": [
          {
            "name": "model_name",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "用于预测的模型名称",
              "title": "Model Name"
            },
            "description": "用于预测的模型名称"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SingleMatchPredictionRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PredictionResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/predict/batch": {
      "post": {
        "tags": [
          "Predictions"
        ],
        "summary": "Predict Batch Matches",
        "description": "批量比赛预测",
        "operationId": "predict_batch_matches_api_v1_predict_batch_post",
        "parameters": [
          {
            "name": "model_name",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "用于预测的模型名称",
              "title": "Model Name"
            },
            "description": "用于预测的模型名称"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BatchMatchPredictionRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BatchPredictionResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/history": {
      "get": {
        "tags": [
          "Predictions"
        ],
        "summary": "Get Prediction History",
        "description": "获取预测历史记录",
        "operationId": "get_prediction_history_api_v1_history_get",
        "parameters": [
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "maximum": 100,
              "minimum": 1,
              "description": "返回结果数量限制",
              "default": 10,
              "title": "Limit"
            },
            "description": "返回结果数量限制"
          },
          {
            "name": "offset",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "minimum": 0,
              "description": "结果偏移量",
              "default": 0,
              "title": "Offset"
            },
            "description": "结果偏移量"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": true,
                  "title": "Response Get Prediction History Api V1 History Get"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/health": {
      "get": {
        "summary": "Health Check",
        "description": "健康检查端点\n\n检查系统各组件的健康状态:\n- API服务状态\n- 数据库连接\n- Redis连接\n- 模型注册表",
        "operationId": "health_check_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthResponse"
                }
              }
            }
          }
        }
      }
    },
    "/metrics": {
      "get": {
        "summary": "Get Metrics",
        "description": "获取系统指标 - Prometheus格式\n\n提供系统运行状态的关键指标, 包括:\n- API请求统计\n- 模型预测统计\n- 系统资源使用\n- 数据管道状态",
        "operationId": "get_metrics_metrics_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/version": {
      "get": {
        "summary": "Get Version",
        "description": "获取版本信息",
        "operationId": "get_version_version_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VersionResponse"
                }
              }
            }
          }
        }
      }
    },
    "/livez": {
      "get": {
        "tags": [
          "health"
        ],
        "summary": "Livez",
        "description": "Liveness probe to check if the application is running.",
        "operationId": "livez_livez_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "type": "object",
                  "title": "Response Livez Livez Get"
                }
              }
            }
          }
        }
      }
    },
    "/readyz": {
      "get": {
        "tags": [
          "health"
        ],
        "summary": "Readyz",
        "description": "Readiness probe to check if the app is ready to serve traffic.",
        "operationId": "readyz_readyz_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "type": "object",
                  "title": "Response Readyz Readyz Get"
                }
              }
            }
          }
        }
      }
    },
    "/": {
      "get": {
        "summary": "Root",
        "description": "根路径",
        "operationId": "root__get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "type": "object",
                  "title": "Response Root  Get"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BatchMatchPredictionRequest": {
        "properties": {
          "matches": {
            "items": {
              "$ref": "#/components/schemas/SingleMatchPredictionRequest"
            },
            "type": "array",
            "minItems": 1,
            "title": "Matches",
            "description": "比赛列表"
          }
        },
        "type": "object",
        "required": [
          "matches"
        ],
        "title": "BatchMatchPredictionRequest",
        "description": "批量比赛预测请求"
      },
      "BatchPredictionResponse": {
        "properties": {
          "predictions": {
            "items": {
              "$ref": "#/components/schemas/PredictionResponse"
            },
            "type": "array",
            "title": "Predictions",
            "description": "预测结果列表"
          },
          "total_matches": {
            "type": "integer",
            "title": "Total Matches",
            "description": "总比赛数"
          },
          "processed_at": {
            "type": "string",
            "format": "date-time",
            "title": "Processed At",
            "description": "处理时间"
          }
        },
        "type": "object",
        "required": [
          "predictions",
          "total_matches",
          "processed_at"
        ],
        "title": "BatchPredictionResponse",
        "description": "批量预测响应模型"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "HealthResponse": {
        "properties": {
          "status": {
            "type": "string",
            "title": "Status"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "title": "Timestamp"
          },
          "version": {
            "type": "string",
            "title": "Version"
          },
          "components": {
            "additionalProperties": true,
            "type": "object",
            "title": "Components"
          }
        },
        "type": "object",
        "required": [
          "status",
          "timestamp",
          "version",
          "components"
        ],
        "title": "HealthResponse",
        "description": "健康检查响应模型"
      },
      "PredictionResponse": {
        "properties": {
          "prediction_id": {
            "type": "string",
            "title": "Prediction Id",
            "description": "预测ID"
          },
          "home_team": {
            "type": "string",
            "title": "Home Team",
            "description": "主队"
          },
          "away_team": {
            "type": "string",
            "title": "Away Team",
            "description": "客队"
          },
          "match_date": {
            "type": "string",
            "format": "date",
            "title": "Match Date",
            "description": "比赛日期"
          },
          "predicted_outcome": {
            "type": "string",
            "title": "Predicted Outcome",
            "description": "预测结果"
          },
          "confidence": {
            "type": "number",
            "maximum": 1.0,
            "minimum": 0.0,
            "title": "Confidence",
            "description": "置信度"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At",
            "description": "创建时间"
          }
        },
        "type": "object",
        "required": [
          "prediction_id",
          "home_team",
          "away_team",
          "match_date",
          "predicted_outcome",
          "confidence",
          "created_at"
        ],
        "title": "PredictionResponse",
        "description": "预测响应模型"
      },
      "SingleMatchPredictionRequest": {
        "properties": {
          "home_team": {
            "type": "string",
            "title": "Home Team",
            "description": "主队名称",
            "examples": [
              "Arsenal"
            ]
          },
          "away_team": {
            "type": "string",
            "title": "Away Team",
            "description": "客队名称",
            "examples": [
              "Chelsea"
            ]
          },
          "match_date": {
            "type": "string",
            "format": "date",
            "title": "Match Date",
            "description": "比赛日期",
            "examples": [
              "2025-08-30"
            ]
          },
          "home_odds": {
            "type": "number",
            "exclusiveMinimum": 1.0,
            "title": "Home Odds",
            "description": "主胜赔率",
            "examples": [
              2.1
            ]
          },
          "draw_odds": {
            "type": "number",
            "exclusiveMinimum": 1.0,
            "title": "Draw Odds",
            "description": "平局赔率",
            "examples": [
              3.3
            ]
          },
          "away_odds": {
            "type": "number",
            "exclusiveMinimum": 1.0,
            "title": "Away Odds",
            "description": "客胜赔率",
            "examples": [
              3.2
            ]
          }
        },
        "type": "object",
        "required": [
          "home_team",
          "away_team",
          "match_date",
          "home_odds",
          "draw_odds",
          "away_odds"
        ],
        "title": "SingleMatchPredictionRequest",
        "description": "单场比赛预测请求"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      },
      "VersionResponse": {
        "properties": {
          "api_version": {
            "type": "string",
            "title": "Api Version"
          },
          "model_version": {
            "type": "string",
            "title": "Model Version"
          },
          "model_info": {
            "additionalProperties": true,
            "type": "object",
            "title": "Model Info"
          }
        },
        "type": "object",
        "required": [
          "api_version",
          "model_version",
          "model_info"
        ],
        "title": "VersionResponse",
        "description": "版本信息响应"
      }
    }
  }
}
